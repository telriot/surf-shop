<h1>New Post</h1>

<form action="/posts" method="POST" enctype="multipart/form-data">
  <div>
    <input name="post[title]" type="text" placeholder="Title" />
  </div>
  <div>
    <input name="post[price]" type="text" placeholder="Price" />
  </div>
  <div>
    <input type="file" accept="images/*" name="images" multiple />
  </div>
  <div>
    <textarea name="post[description]" placeholder="Description"></textarea>
  </div>
  <div>
    <input name="post[location]" type="text" placeholder="Location" />
  </div>
  <input type="submit" />
</form>

